<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xmlns:tx="http://www.springframework.org/schema/tx"
	 xsi:schemaLocation="
	 http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
	 http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd" 
	 default-autowire="byName">


	<bean id="myCustomDeviceFactory" class="****** YOUR CUSTOM DEVICE FACTORY ******" />

	<bean id="pushNotificationManager" class="javapns.notification.PushNotificationManager" scope="singleton"><property name="deviceFactory" ref="myCustomDeviceFactory" /></bean>
	<bean id="feedbackServiceManager" class="javapns.feedback.FeedbackServiceManager" scope="singleton"><property name="deviceFactory" ref="myCustomDeviceFactory" /></bean>

</beans>

